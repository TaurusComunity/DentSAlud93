---
import Navegacion from "../components/Navegacion.astro";
import Titulos from "../components/Titulos.astro";

interface Props {
	titulo: string;
}

const { titulo } = Astro.props;
---

<header
  class="header invisible bg-gradient-to-b from-azul_oscuro via-azul_rey to-azul_claro  px-[80px] rounded-b-[45px]"
>
  <Navegacion />
  <Titulos titulos={titulo} />
</header>
